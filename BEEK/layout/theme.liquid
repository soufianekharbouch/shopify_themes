                        jQuery(productLink).addClass('with__label');
                        jQuery(productLink)
                          .append('<div class="product__label">' + label + '</div>');
                      } else {
                        return;
                      }
                    }).catch((err) => console.log(err,'LimeSpot err while getting request'));
                })(i);
              }
            });
          },1);
