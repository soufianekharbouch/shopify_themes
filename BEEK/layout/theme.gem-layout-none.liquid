<script>
//  need to create task in order to fully load LimeSpot methods
// or add if to check whether it exists
  setTimeout(() => {
    LimeSpot.ListenOnce && LimeSpot.ListenOnce(document, "lsRecommendationsLoaded", function() {
  
        var productLinks = document.querySelectorAll(".ls-recommendation-box:not([data-box-type='Upsell']) .ls-link");
        for (var i = 0; i < productLinks.length; i++) {
          (function(i) {
            var productLink = productLinks[ i ];
  
            var href = productLink.href;
    
            if (!href) return;
            var handle = LimeSpot.Shopify.GetProductHandle(href);
  
            const req = jQuery.ajax({
              type: "GET",
              url: "/products/" + handle + "?view=accentuate-product-label.json"
            }).done(function(label) {
               console.log('label when done',label)
                if (label !== "") {
                  jQuery(productLink).addClass('with__label');
                  jQuery(productLink)
                    .append('<div class="product__label">' + label + '</div>');
                } else {
                  return;
                }
              }).catch((err) => console.log(err,'LimeSpot err while getting request'));
  
  
          })(i);
        }
      });
    },1);
</script>


