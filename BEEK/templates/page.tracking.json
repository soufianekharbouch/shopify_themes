    "faq": {
      "type": "wonderment-faq",
      "blocks": {
        "shipping-heading": {
          "type": "section_heading",
          "settings": {
            "title": "FAQs"
          }
        },
        "when-shipping-question": {
          "type": "question",
          "settings": {
            "title": "When will my order ship?",
            "text": "<p>We ship Monday-Friday, 9am-5pm EST; weekends or US holidays do not count as business days. <\/p>"
          }
        },
        "when-arriving-question": {
          "type": "question",
          "settings": {
            "title": "When will my order arrive?",
            "text": "<p>Expected delivery for Standard Shipping is 5-6 business days from the time the order is shipped. Expedited orders ship within 2-3 business days.<\/p>"
          }
        },
        "return-policy-question": {
          "type": "question",
          "settings": {
            "title": "What is your return policy?",
            "text": "<p>We want every customer (or as we call you, Neighbor) to be entirely thrilled with their Beekman 1802 purchase. But if you’re not thrilled, we are happy to accept returns from purchases made at Beekman1802.com or from the Beekman 1802 Kindness Shop in Sharon Springs, NY.<br\/><br\/>Beekman 1802 accepts returns of items purchased directly from beekman1802.com within 30 days of the delivery date. To start the return process with Beekman 1802, reach out to <a href=\"mailto:Neighborservices@beekman1802.com\">Neighborservices@beekman1802.com<\/a>. The Neighbor Services Team will email you a shipping label to print off and provide you with a return authorization number.<\/p>"
          }
        },
        "ship-international-question": {
          "type": "question",
          "settings": {
            "title": "Can I visit\/tour the farm?",
            "text": "<p>The farm is open occasionally for tours. Please sign up for our email newsletter for announcements on future tours. Because Beekman 1802 Farm is a private residence and working farm, we must restrict access to Farm Tour dates only.<\/p>"
          }
        },
        "shipping-cost-question": {
          "type": "question",
          "settings": {
            "title": "Where is the Beekman 1802 Kindness Shop?",
            "text": "<p>Beekman 1802 Kindness Shop is located at 187 Main Street in Sharon Springs, NY 13459. We are open 7 days a week from 10 AM- 6 PM (except Christmas, Thanksgiving & New Years.)<\/p>"
          }
        }
      },
