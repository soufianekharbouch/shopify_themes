<script>
  $('.js-plus-amount').click(function(){
    qty=Number($(this).next('.item-amount').val())+1
    line=$(this).parent().attr('data-line')
    $.ajax({
              url: "cart/change.js",
              dataType: 'json',
              type: 'post',
              data: {'line': line,'quantity': qty},
              success: function(data) { 
               console.log(data)
               const myTimeout = setTimeout(function(){ _addGiftCart(data);}, 1000);
              },
              error: function(XMLHttpRequest) {
              }
            });
  })

  $('.js-minus-amount').click(function(){
    qty=Number($(this).parent().find('.item-amount').val())-1
    line=$(this).parent().attr('data-line')
    $.ajax({
              url: "cart/change.js",
              dataType: 'json',
              type: 'post',
              data: {'line': line,'quantity': qty},
              success: function(data) { 
               console.log(data)
               const myTimeout = setTimeout(function(){ _addGiftCart(data);}, 1000);
              },
              error: function(XMLHttpRequest) {
              }
            });
  })


  $('.remove-button').click(function(){

  line=$(this).attr('data-line')
  my=$(this);
    $.ajax({
              url: "cart/change.js",
              dataType: 'json',
              type: 'post',
              data: {'line': line,'quantity': 0},
              success: function(data) { 
               my.parents(".row").remove();
               const myTimeout = setTimeout(function(){ _addGiftCart(data);}, 1000);
              },
              error: function(XMLHttpRequest) {
                console.log(XMLHttpRequest)
              }
            });
  })
  

  function _addGiftCart(data){
        var variant_id='{{settings.free_gift_variant_id}}';
        var ammount={{settings.free_gift_ammount}};
        var Gquantity={{settings.free_gift_quantity}};
        if(!{{settings.free_gift_promo_enabled}}) 
        return;
        $.ajax({
            url: "/cart.js",
            success: function(cart) { 
              cartData=JSON.parse(cart)
              t=0;
              var total=0;
              for(item of cartData.items){
                console.log(item)
                if(item.variant_id == variant_id || item.product_type==="Gift Card" || item.product_type==="Event" ){
                    total=Number(total+item.final_line_price);
                    if(item.variant_id == variant_id)
                    t=1;
                     
                  }
                }
               if((cartData.item_count>0 && Number(cartData.total_price-total)<Number(ammount*100)) || cartData.item_count==0 )
               {
                 j=1;
                for(item of cartData.items){
                  if(item.variant_id == variant_id ){
