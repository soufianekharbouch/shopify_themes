<div id="sca_fg_cart">
  <div class="container cart-page">
    <h1 class="page-title">{{ 'cart.general.title' | t }}</h1>
    {% if cart.item_count > 0 %}
      <div class="gift__wrapper">
        <div class="row header-product-block">
          <div class="col-12 product-block-wrap">
            <div id="sca-fg-owl-carousel">
              <div id="freegift_cart_container" class="sca-fg-cart-container">
                <div id="freegift_message" class="owl-carousel"></div>
              </div>
              <div id="sca-fg-slider-cart" style="display: none;">
                <div class="sca-fg-cart-title">
                  <strong>SELECT A FREE SAMPLE</strong>
                </div>
                <div id="sca_freegift_list" class="owl-carousel"></div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <form action="/cart" method="post" novalidate>
        <div class="row header-product-block">
          <div class="col-12 product-block-wrap">
            <div class="product-block">
              <p class="title">Shopping cart ({{ cart.item_count }} product{{ prefix }})</p>
            </div>
            <div class="quantity-block">
              <p class="title">{{ 'cart.label.product' | t }}</p>
            </div>
            <div class="price-block">
              <p class="title">{{ 'cart.label.price' | t }}</p>
            </div>
          </div>
        </div>

        {% for item in cart.items %}
          {% unless item.id == giftItemId %}
            <div class="row">
              <div class="col-12 product-block-wrap product__wrap" data-variant-id="{{ item.variant_id }}">
                <div class="product-block">
                  <div class="img__wrap d-none d-sm-block">
                    <a href="{{ item.url | within: collections.all }}">
                      {% include 'responsive-image' with
                        image: item.image,
                        max_width_image: 127,
                        max_height_image: 170,
                        image_attributes: "data-product-featured-image",
                        wrapper_class: wrapper_class,
                        wrapper_attributes: "data-product-image-wrapper",%}
                    </a>
                  </div>
                  <div class="img__wrap d-sm-none">
                    <a href="{{ item.url | within: collections.all }}">
                      {% include 'responsive-image' with
                        image: item.image,
                        max_width_image: 76,
                        max_height_image: 92,
                        image_attributes: "data-product-featured-image",
                        wrapper_class: wrapper_class,
                        wrapper_attributes: "data-product-image-wrapper",%}
                    </a>
                  </div>
                  <div class="details-wrap">
                    {% if item.product.metafields.accentuate.parent_product != blank %}
                      {% assign parent_prod = item.product.metafields.accentuate.parent_product %}
                      {% assign parent_prod = all_products[parent_prod] %}
                      <a href="{{ parent_prod.url }}" class="product-title">
                        {{ parent_prod.metafields.accentuate.custom_name | default: parent_prod.title }}
                      </a>
                    {% else %}
                      <a href="{{ item.url }}" class="product-title">
                        {{ item.product.metafields.accentuate.custom_name | default: item.product.title }}
                      </a>
                    {% endif %}
                    {% assign hasVariants = false %}
                    {% unless item.product.has_only_default_variant %}
                      {% unless item.variant.title == 'Default Title' %}
                      {% assign hasVariants = true %}
                      {% endunless %}
                    {% endunless %}

                    {% if hasVariants %}
                      <p class="desc">{{ item.product.metafields.accentuate.short_description }}</p>
                      <p class="variant d-none d-sm-block">{{ item.variant.title }}</p>
                    {% else %}
                      <p class="desc last">{{ item.product.metafields.accentuate.short_description }}</p>
                    {% endif %}
                    <div class="amount js-amount-block" data-line="{{ forloop.index }}">
                      <span class="js-minus-amount">-</span>
                      <div class="input-amount js-amount-block">
                        {{ item.quantity }}
                      </div>
                      <span class="js-plus-amount">+</span>
                    </div>
                    <a href="#" class="remove-button" data-line="{{ forloop.index }}">
                      {{ 'cart.general.remove' | t }}
                    </a>
                  </div>
                </div>
                <div class="quantity-block">
                  <div class="amount js-amount-block" data-line="{{ forloop.index }}">
                    <span class="js-minus-amount">-</span>
                    <div class="input-amount js-amount-block">
                      {{ item.quantity }}
                    </div>
                    <span class="js-plus-amount">+</span>

                    <input type="hidden"
                           name="updates[]"
                           class="item-amount"
                           id="updates_{{ item.key }}"
                           value="{{ item.quantity }}"
                           min="0"
                           aria-label="{{ 'cart.general.item_quantity' | t }}">
                  </div>
                  <!--
                  <input class="UpdateQtyCartPage" data-line="{{forloop.index}}"  type="submit" name="update" onclick="return false;" value="update">
                  -->
                </div>
                <div class="price-block">
                  <p class="price">{{ item.line_price | money }}</p>
                  <input type="submit" name="update" value="update">
                </div>
              </div>
            </div>
          {% endunless %}
        {% endfor %}

        {% for item in cart.items %}
          {% if item.id == giftItemId %}
            <div class="row">
              <div class="col-12 product-block-wrap gift_product_wrap product__wrap">
                <div class="product-block">
                  <div class="img__wrap">
                    <a href="{{ item.url | within: collections.all }}">
                      {% include 'responsive-image' with
                        image: item.image,
                        max_width_image: 127,
                        max_height_image: 170,
                        image_attributes: "data-product-featured-image",
                        wrapper_class: wrapper_class,
                        wrapper_attributes: "data-product-image-wrapper",%}
                    </a>
                  </div>

                  <div class="details-wrap">
                    <a href="{{ item.url }}" class="product-title">
                      {{ item.product.metafields.accentuate.custom_name | default: item.product.title }}
                    </a>
                    <p>{{ item.product.metafields.accentuate.short_description }}</p>
                    <p class="free__gift-message desc">{{ item.product.description }}</p>
                    <a href="" class="remove-button" data-line="{{ forloop.index }}">
                      Change
                    </a>
                  </div>
                </div>
                <div class="price-block block__copy">
                  <div class="d-none d-sm-block price">Free</div>
                  <div class="d-sm-none price">{{ item.product.price | money }}</div>
                </div>
              </div>
            </div>
          {% endif %}
        {% endfor %}

        {%- capture taxes_shipping_checkout -%}
          {%- if shop.taxes_included and shop.shipping_policy.body != blank -%}
            {{ 'cart.general.taxes_included_and_shipping_policy_html' | t: link: shop.shipping_policy.url }}
            {%- elsif shop.taxes_included -%}
            {{ 'cart.general.taxes_included_but_shipping_at_checkout' | t }}
            {%- elsif shop.shipping_policy.body != blank -%}
            {{ 'cart.general.taxes_and_shipping_policy_at_checkout_html' | t: link: shop.shipping_policy.url }}
          {%- else -%}
            {{ 'cart.general.taxes_and_shipping_at_checkout' | t }}
          {%- endif -%}
        {%- endcapture -%}

        <div class="row d-flex justify-content-start free-shipping-wrap">
          <div class="col-lg-5">
            <div class="col-lg-6 free-shipping">
              <div class="icon-shipping-block">
                {% include 'icon-shipping' %}
              </div>
              {% assign free_text = shop.metafields.accentuate.free_cart_text  | newline_to_br | split: '<br />' %}
              {% for paragraph in free_text %}
                <p>{{ paragraph }}</p>
              {% endfor %}
            </div>
          </div>
          <div class="col-lg-5">
            {% if settings.cart_notes_enable %}
              <div class="row d-flex justify-content-end">
                <div class="col-lg-12 justify-content-between subtotal-block">
                  <p class="title">
                    <label for="CartSpecialInstructions" class="w-100">{{ 'cart.general.note' | t }}</label>
                  </p>
                  <textarea name="note" maxlength="250" id="CartSpecialInstructions" class="w-100">{{ cart.note }}</textarea>
                </div>
              </div>
            {% endif %}

            <div class="row d-flex justify-content-end">
              <div class="col-lg-12 d-flex justify-content-between subtotal-block">
              <span class="title">
                {{ 'cart.general.subtotal' | t }}
              </span>
                <span class="total-price">
                {{ cart.total_price | money }}
              </span>
              </div>
            </div>

            <div class="row d-flex justify-content-end">
              <div class="col-md-12 col-lg-12 promo-code-block">
                <p>
                  {{ 'cart.general.shipping_at_checkout' | t }}
                </p>
              </div>
            </div>

            <div class="row d-flex justify-content-end">
              <div class="col-lg-12 d-flex justify-content-between total-block">
                <div class="total__wrap d-flex justify-content-between w-100">
                <span class="title">
                  total
                </span>
                  <span class="total-price">
                  {{ cart.total_price | money }}
                </span>
                </div>
              </div>
            </div>
          </div>
        </div>

        {% if cart.total_discounts > 0 %}
          <p>{{ 'cart.general.savings' | t }} {{ cart.total_discounts | money }}</p>
        {% endif %}

        <div class="row cart-actions-block">
          <div class="col-12 col-md-6 d-flex align-items-center continue-shopping-block">
            <a id="continue-shopping" href="" class="continue-link" title="Continue shopping">Continue shopping</a>
          </div>
          <div class="col-12 col-md-6 d-flex justify-content-end checkout-block">
            <input type="submit" name="checkout" value="{{ 'cart.general.checkout' | t }}">
          </div>
          

        </div>
      </form>

      {% if settings.cart_crosssell_enable %}
        <div class="row most__loved-wrap">
          {% include 'cart_crossell_products' with cartproducts: cart.items %}
        </div>
      {% endif %}

    {% else %}
      <div class="supports-cookies">
        <p>{{ 'cart.general.empty' | t }}</p>
        <p>{{ 'cart.general.continue_browsing_html' | t }}</p>
      </div>
    {% endif %}
  </div>

