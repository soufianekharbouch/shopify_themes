  }

  #handle1 {
  position: absolute;
  height: 50px;
  width: 50px;
  top: 50%;
  left: 50%;
  transform: translateY(-50%) translateX(-50%);
  z-index: 1;
    
  display: block!important;
}
    
 #handle1::before {
    border-right: 6px solid white;
    left: 50%;
    margin-left: -17px;
  }
   #handle1::after {
    border-left: 6px solid white;
    right: 50%;
    margin-right: -17px;
  }


#comparisons input[type=range]{
	-webkit-appearance:none;
