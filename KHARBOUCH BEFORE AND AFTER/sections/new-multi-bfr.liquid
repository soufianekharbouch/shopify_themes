#handle1:before, #handle1:after {
    content: '';
    width: 0;
    height: 0;
    border: 6px inset transparent;
    position: absolute;
    top: 53%;
    margin-top: -6px;
}
#comparisons {
    height: 400px;
    padding-bottom: unset!important;
    position: relative;
    margin: 0 auto;
  border-radius:10px;
  
}

#comparisons figure { 
  position: absolute;
	//background-image: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/4273/photoshop-face-before.jpg);
	background-size: cover;
	font-size: 0;
	width: 100%;
	height: 100%;
	margin: 0; 
    overflow:hidden;
  border-radius:10px;
  
}
  
  #divisor1 { 
	//background-image: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/4273/photoshop-face-after.jpg);
	background-size: cover;
	position: absolute;
	width: 50%; 
	box-shadow: 0 5px 10px -2px rgba(0,0,0,0.3);
	bottom: 0; height: 100%;
  display: block!important;
   }
 #divisor1::before,  #divisor1::after {
    content: '';
    position: absolute;
    right: -2px;
    width: 4px;
    height: calc(50% - 23px);
    background: white;
    z-index: 2;
  }
 #divisor1::before {
    top: 0;
