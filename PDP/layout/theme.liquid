    {% if template.name == 'product' and settings.sticky-add-cart == true %}
      <style>
        sticky-add-cart {
        display: block;
        width: 100%;
        padding: 10px 20px;
        background-color: #fff;
        position: fixed;
        bottom: 0;
        z-index: 99;
        box-shadow: 0 0 6px #000;
      }
      .sticky-add-cart-wrap {
        display: flex;
        flex-direction: column;
        gap: 10px;
        align-items: center;
      }
      .sticky-add-cart-wrap div {
        font-size: 20px;
        font-weight: 400;
        line-height: 1.125;
        color: #000;
      }
      .sticky-add-cart-wrap button {
        font-size: 18px !important;
        background-color: #000;
      }
      </style>
      <sticky-add-cart class="hidden" data-sticky="sticky-{{ product.id }}">
      {% form 'product', product %}
        <input type="hidden" name="id" value={{ product.selected_or_first_available_variant.id }}>
        <div class="sticky-add-cart-wrap">
          <div>{{ product.title }}</div>
          <button type="submit" name="add" class="button {% if settings.cart_action == 'mini_cart' or settings.cart_action == 'drawer' %} ajax-submit {% endif %}action_button button--add-to-cart" data-add-to-cart-trigger>
              <span class="text">Add to Cart</span>
              <svg x="0px" y="0px" width="32px" height="32px" viewBox="0 0 32 32" class="checkmark">
                <path fill="none" stroke-width="2" stroke-linecap="square" stroke-miterlimit="10" d="M9,17l3.9,3.9c0.1,0.1,0.2,0.1,0.3,0L23,11"/>
              </svg>
            </button>
        </div>
      {% endform %}
      </sticky-add-cart>
      <script>
      let showStickyBox = document.querySelector('sticky-add-cart');
      let getStickyBox = document.querySelector(` button[data-sticky="${showStickyBox.dataset.sticky}"]`);
      window.addEventListener('scroll', ()=>{
        if (window.innerWidth < 767) {
          let getTop = getStickyBox.offsetTop + (getStickyBox.offsetHeight * 2);
          let topped = document.querySelector('html').scrollTop;
          if (getTop > topped) {
            showStickyBox.classList.add('hidden');
          } else {
            showStickyBox.classList.remove('hidden');
          }
        }
      });
      </script>
    {% endif %}
